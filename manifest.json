{
  "_version": "1.58.0",
  "sap.app": {
    "id": "com.рекс.cryptotrading",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.1.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "dataSources": {
      "cryptoAPI": {
        "uri": "/api/",
        "type": "REST"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://Fiori2/F0002",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.suite.ui.commons": {},
        "sap.viz": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.рекс.cryptotrading.i18n.i18n"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.рекс.cryptotrading.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "launchpad",
          "pattern": "",
          "target": "launchpad"
        }
      ],
      "targets": {
        "launchpad": {
          "viewName": "App",
          "viewLevel": 1
        }
      }
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "рекс-cryptotrading"
  },
  "sap.platform.hcp": {
    "name": "рекс-cryptotrading",
    "appName": "",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProjectCommon",
      "version": "1.4.0"
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "analytical"
  }
}