{
  "name": "com-rex-trading-launchpad",
  "version": "1.0.0",
  "description": "rex.com - Professional Crypto Trading Platform - SAP Fiori Launchpad",
  "main": "app.py",
  "scripts": {
    "start": "ui5 serve --port 8080",
    "start:simple": "ui5 serve --config ui5-simple.yaml --port 8080",
    "build": "ui5 build --dest dist --all",
    "build:prod": "ui5 build --dest dist --minify",
    "clean": "rimraf dist node_modules/.cache",
    "lint": "eslint webapp/ --ext .js",
    "dev": "concurrently \"python3 app.py\" \"npm run start\""
  },
  "keywords": [
    "sap",
    "ui5",
    "fiori",
    "launchpad",
    "crypto",
    "trading",
    "enterprise"
  ],
  "author": "rex.com",
  "license": "MIT",
  "devDependencies": {
    "@ui5/cli": "^4.0.9",
    "@ui5/builder": "^4.0.9", 
    "@ui5/server": "^4.0.6",
    "@ui5/project": "^4.0.4",
    "@ui5/fs": "^4.0.1",
    "@ui5/logger": "^4.0.1",
    "eslint": "^8.57.0",
    "rimraf": "^3.0.2",
    "concurrently": "^7.6.0"
  },
  "ui5": {
    "dependencies": [
      "@openui5/sap.ui.core",
      "@openui5/sap.m", 
      "@openui5/sap.f",
      "@openui5/themelib_sap_horizon"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
