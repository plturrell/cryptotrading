{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "MCP_ENVIRONMENT": "production",
    "MCP_TRANSPORT": "http",
    "MCP_REQUIRE_AUTH": "true",
    "MCP_RATE_LIMIT_GLOBAL": "1000",
    "MCP_STRICT_VALIDATION": "true",
    "MCP_AUDIT_LOGGING": "true"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  }
}