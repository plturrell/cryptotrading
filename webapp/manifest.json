{
    "_version": "1.120.0",
    "sap.app": {
        "id": "com.rex.cryptotrading",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "ach": "FIN-FSCM-TRM",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.11.0",
            "toolsId": "crypto-trading-enterprise"
        },
        "crossNavigation": {
            "inbounds": {
                "launchpad": {
                    "semanticObject": "CryptoTrading",
                    "action": "display",
                    "title": "rex.com Launchpad",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                },
                "bitcoin": {
                    "semanticObject": "Bitcoin",
                    "action": "analyze",
                    "title": "Bitcoin Analytics",
                    "signature": {
                        "parameters": {
                            "symbol": {
                                "defaultValue": {
                                    "value": "BTC"
                                }
                            }
                        }
                    }
                },
                "wallet": {
                    "semanticObject": "Wallet",
                    "action": "manage",
                    "title": "Wallet Management",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                },
                "trading": {
                    "semanticObject": "Trading",
                    "action": "execute",
                    "title": "Trading Console",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                },
                "technicalAnalysis": {
                    "semanticObject": "TechnicalAnalysis",
                    "action": "analyze",
                    "title": "Technical Analysis Dashboard",
                    "signature": {
                        "parameters": {
                            "symbol": {
                                "defaultValue": {
                                    "value": "BTC-USD"
                                }
                            }
                        },
                        "additionalParameters": "allowed"
                    }
                }
            }
        },
        "dataSources": {
            "mainService": {
                "uri": "/api/v1/crypto/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0",
                    "localUri": "localService/metadata.xml"
                }
            },
            "marketDataService": {
                "uri": "/api/v1/market/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            },
            "tradingService": {
                "uri": "/api/v1/trading/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            },
            "portfolioService": {
                "uri": "/api/v1/portfolio/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            },
            "analyticsService": {
                "uri": "/api/v1/analytics/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            },
            "riskService": {
                "uri": "/api/v1/risk/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            },
            "walletService": {
                "uri": "/api/wallet/",
                "type": "REST"
            },
            "webSocketService": {
                "uri": "/ws/market-data",
                "type": "WebSocket"
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://business-card",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "config": {
            "sapFiori2Adaptation": true
        },
        "rootView": {
            "viewName": "com.rex.cryptotrading.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.ushell": {},
                "sap.ui.fl": {},
                "sap.ui.comp": {},
                "sap.ui.table": {},
                "sap.uxap": {},
                "sap.ui.layout": {},
                "sap.viz": {},
                "sap.suite.ui.microchart": {},
                "sap.ui.unified": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.rex.cryptotrading.i18n.i18n",
                    "supportedLocales": ["en", "de", "fr", "es", "zh", "ja"],
                    "fallbackLocale": "en"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "synchronizationMode": "None",
                    "groupId": "$auto",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "marketData": {
                "dataSource": "marketDataService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "synchronizationMode": "None",
                    "groupId": "$auto",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "trading": {
                "dataSource": "tradingService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "synchronizationMode": "None",
                    "groupId": "$auto",
                    "autoExpandSelect": true,
                    "earlyRequests": false
                }
            },
            "portfolio": {
                "dataSource": "portfolioService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "synchronizationMode": "None",
                    "groupId": "$auto",
                    "autoExpandSelect": true,
                    "earlyRequests": false
                }
            },
            "analytics": {
                "dataSource": "analyticsService",
                "preload": false,
                "settings": {
                    "operationMode": "Server",
                    "synchronizationMode": "None",
                    "groupId": "$auto",
                    "autoExpandSelect": true,
                    "earlyRequests": false
                }
            },
            "risk": {
                "dataSource": "riskService",
                "preload": false,
                "settings": {
                    "operationMode": "Server",
                    "synchronizationMode": "None",
                    "groupId": "$auto",
                    "autoExpandSelect": true,
                    "earlyRequests": false
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/designTokens.css"
                },
                {
                    "uri": "css/sharedComponents.css"
                },
                {
                    "uri": "css/style.css"
                },
                {
                    "uri": "css/loadingStates.css"
                },
                {
                    "uri": "css/tradingTheme.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "com.rex.cryptotrading.view",
                "controlId": "navContainer",
                "controlAggregation": "pages",
                "transition": "slide",
                "bypassed": {
                    "target": "notFound"
                },
                "async": true
            },
            "routes": [
                {
                    "name": "dashboard",
                    "pattern": "",
                    "target": "dashboard"
                },
                {
                    "name": "market",
                    "pattern": "market",
                    "target": "market"
                },
                {
                    "name": "marketDetail",
                    "pattern": "market/{symbol}",
                    "target": "marketDetail"
                },
                {
                    "name": "portfolio",
                    "pattern": "portfolio",
                    "target": "portfolio"
                },
                {
                    "name": "trading",
                    "pattern": "trading",
                    "target": "trading"
                },
                {
                    "name": "tradingDetail",
                    "pattern": "trading/{pair}",
                    "target": "tradingDetail"
                },
                {
                    "name": "analytics",
                    "pattern": "analytics",
                    "target": "analytics"
                },
                {
                    "name": "risk",
                    "pattern": "risk",
                    "target": "risk"
                },
                {
                    "name": "wallet",
                    "pattern": "wallet",
                    "target": "wallet"
                },
                {
                    "name": "settings",
                    "pattern": "settings",
                    "target": "settings"
                },
                {
                    "name": "alerts",
                    "pattern": "alerts",
                    "target": "alerts"
                },
                {
                    "name": "history",
                    "pattern": "history",
                    "target": "history"
                }
            ],
            "targets": {
                "dashboard": {
                    "viewType": "XML",
                    "viewName": "Dashboard",
                    "viewLevel": 1
                },
                "market": {
                    "viewType": "XML",
                    "viewName": "Market",
                    "viewLevel": 2
                },
                "marketDetail": {
                    "viewType": "XML",
                    "viewName": "MarketDetail",
                    "viewLevel": 3
                },
                "portfolio": {
                    "viewType": "XML",
                    "viewName": "Portfolio",
                    "viewLevel": 2
                },
                "trading": {
                    "viewType": "XML",
                    "viewName": "Trading",
                    "viewLevel": 2
                },
                "tradingDetail": {
                    "viewType": "XML",
                    "viewName": "TradingDetail",
                    "viewLevel": 3
                },
                "analytics": {
                    "viewType": "XML",
                    "viewName": "Analytics",
                    "viewLevel": 2
                },
                "risk": {
                    "viewType": "XML",
                    "viewName": "Risk",
                    "viewLevel": 2
                },
                "wallet": {
                    "viewType": "XML",
                    "viewName": "Wallet",
                    "viewLevel": 2
                },
                "settings": {
                    "viewType": "XML",
                    "viewName": "Settings",
                    "viewLevel": 2
                },
                "alerts": {
                    "viewType": "XML",
                    "viewName": "Alerts",
                    "viewLevel": 2
                },
                "history": {
                    "viewType": "XML",
                    "viewName": "History",
                    "viewLevel": 2
                },
                "notFound": {
                    "viewType": "XML",
                    "viewName": "NotFound",
                    "viewLevel": 1
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": ["F0001"],
        "archeType": "transactional"
    },
    "sap.cloud": {
        "public": true,
        "service": "com.rex.cryptotrading"
    },
    "sap.platform.cf": {
        "ui5VersionNumber": "1.120.0"
    },
    "sap.flp": {
        "type": "application"
    },
    "sap.card": {
        "type": "analytical",
        "configuration": {
            "destinations": {
                "CryptoService": {
                    "name": "CryptoService"
                }
            }
        }
    },
    "sap.pwa": {
        "enabled": true,
        "appShortName": "Crypto Trading",
        "appName": "Enterprise Crypto Trading Platform",
        "description": "Professional cryptocurrency trading and portfolio management platform",
        "startUrl": "./",
        "scope": "./",
        "display": "standalone",
        "orientation": "portrait-primary",
        "themeColor": "#0070f2",
        "backgroundColor": "#ffffff",
        "icons": [
            {
                "src": "./resources/pwa/icon-192.png",
                "sizes": "192x192",
                "type": "image/png"
            },
            {
                "src": "./resources/pwa/icon-512.png",
                "sizes": "512x512",
                "type": "image/png"
            }
        ]
    }
}